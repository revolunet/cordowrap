{
  "name": "cordowrap",
  "version": "0.0.1",
  "description": "Promise-based Cordova APIs with mocks support",
  "license": "MIT",
  "repository": "revolunet/cordowrap",
  "author": {
    "name": "Julien Bouquillon",
    "email": "julien@bouquillon.com",
    "url": "http://github.com/revolunet"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "readme": "node ./node_modules/node-readme/bin/node-readme.js",
    "test": "find ./spec -iname '*.spec.js' -exec ./node_modules/.bin/babel-node {} \\; | ./node_modules/.bin/tap-spec",
    "zuul": "./node_modules/zuul/bin/zuul -- spec/**/*.spec.js",
    "build": "babel -d ./dist ./src",
    "watch": "babel --watch -d ./dist ./src"
  },
  "main": "dist/index.js",
  "keywords": [
    "cordova",
    "phonegap",
    "api",
    "promise"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel": "5.6.23",
    "babel-eslint": "3.1.23",
    "babelify": "6.1.2",
    "es6-template-strings": "1.0.0",
    "eslint": "1.0.0-rc-1",
    "eslint-config-airbnb": "0.0.6",
    "node-readme": "0.1.7",
    "semistandard": "6.1.2",
    "tap-spec": "4.0.2",
    "tape": "4.0.0",
    "zuul": "3.2.0"
  }
}
